{
    "name": "growthoptimized/optimizely-sdk",
    "type": "library",
    "description": "Optimizely API PHP SDK",
    "license": "MIT",
    "authors": [
        {
            "name": "Valentin Prugnaud",
            "email": "valentin.prugnaud@widerfunnel.com",
            "homepage": "https://github.com/GrowthOptimized/Optimizely-SDK"
        }
